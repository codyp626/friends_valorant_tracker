@page "/"

<h1>Home</h1>

<button class="btn btn-primary" @onclick="GotoRank" >Go to Rank Page</button>

@code {
    [Inject]
    private NavigationManager NavigationManager { get; set; } = null!;

    @* private void GotoMongo()
    {
        NavigationManager.NavigateTo("/mongo");
    } *@
    private void GotoRank()
    {
        NavigationManager.NavigateTo("/rank");
    }
}